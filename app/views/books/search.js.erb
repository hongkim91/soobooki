<% if params[:api] == "google" %>
   $('.search-results').html("<%= j render "search_google" %>")
<% elsif params[:api] == "daum" %>
   $('.search-results').html("<%= j render "search_daum" %>")
<% else %>
   $('.search-results').html("something went wrong. sorry.")
<% end %>
$('.bookshelf-container').hide()
$('.friends-container').hide()
$('.notifications-container').hide()
$('.my-account-container').hide()
$('.search-results').show()
